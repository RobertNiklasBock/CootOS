    "version": "2.0.0",
    "tasks": [
        {
            "type": "cargo",
            "subcommand": "rustc -- -C link-arg=-nostartfiles",
            "problemMatcher": [
                "$rustc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}